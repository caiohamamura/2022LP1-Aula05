<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp1"
    xmlns:vm="clr-namespace:MauiApp1.ViewModel"
    >


    <TabBar >
        <Tab
            Title="Comum"
            Icon="dotnet_bot.png">
            <ShellContent
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
        </Tab>
        <Tab
            Title="Jeans"
            Icon="dotnet_bot.png">
            <ShellContent    
            ContentTemplate="{DataTemplate local:Jeans}"
            Route="Jeans" />
        </Tab>
        <Tab
            Title="CMB"
            Icon="dotnet_bot.png">
            <ShellContent    
            ContentTemplate="{DataTemplate local:CamaMesaBanho}"
            Route="CamaMesaBanho" />
        </Tab>
    </TabBar>
</Shell>
